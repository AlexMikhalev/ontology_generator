[package]
name = "{{ name }}"
version = "{{ version }}"
edition = "2021"
description = "{{ description }}"
license = "MIT"

[dependencies]
ring = { version = "0.17.8", features = ["wasm32_unknown_unknown_js"] }
atomic_lib = { version = "0.40.0", features = ["config"] }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
anyhow = "1.0" 

[build]
target = "wasm32-unknown-unknown-js"